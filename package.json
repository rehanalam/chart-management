{
    "name": "my-monorepo",
    "private": true,
    "workspaces": [
      "web-app",
      "proxy-server"
    ],
    "scripts": {
      "dev:chart": "yarn workspace web-app dev",
      "dev:proxy": "yarn workspace proxy-server dev",
      "dev": "concurrently \"yarn dev:chart\" \"yarn dev:proxy\""
    },
    "devDependencies": {
      "concurrently": "^8.2.0"
    }
  }
  