{
    "name": "my-monorepo",
    "private": true,
    "workspaces": [
      "web-app",
      "proxy-server"
    ],
    "scripts": {
        "dev:chart-management": "yarn workspace chart-management dev",
        "build:chart-management": "yarn workspace chart-management build",
        "lint:chart-management": "yarn workspace chart-management lint",
        "dev:proxy-server": "yarn workspace proxy-server start",
        "start": "concurrently \"yarn dev:chart-management\" \"yarn dev:proxy-server\""    
    },
    "devDependencies": {
      "concurrently": "^8.2.0"
    }
  }
  